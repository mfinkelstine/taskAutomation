* {
  margin: 0;
  padding: 0;
  font-family: Lato;
}
body {
  padding: 0px;
  background: #e5e2e6;
  background-color: #b0c4de;
}
input[type=text] {
  padding: 5px;
  width:100%;
  height:40px;
  border:1px solid #49b637;
  margin:10px 0;
  box-shadow: 0 0 5px rgb(90, 90, 111);
  border-radius:3px;
 font-family: 'Droid Serif', serif;
}

input[type=submit],input[type=reset]{
   background-color:#781149;
   color:white;
   border-radius:5px;
   padding:0;
   width:50%;
   height:40px;
   border:2px solid white;
   font-size:16px;
   font-family: 'Droid Serif', serif;
   text-align:center;
}
input[type=submit]:hover{
   background-color:orange;
   cursor:pointer;
}
input[type=reset]:hover{
   background-color:orange;
   cursor:pointer;
}

